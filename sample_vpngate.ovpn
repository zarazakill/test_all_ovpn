client
dev tun
proto udp
remote 123.456.789.123 1194
resolv-retry infinite
nobind
persist-key
persist-tun
remote-cert-tls server
cipher AES-256-CBC
auth SHA256
verb 3
mute 20
keepalive 10 30

# CA certificate would go here
ca /path/to/ca.crt
# Client certificate would go here
cert /path/to/client.crt
# Client key would go here
key /path/to/client.key
auth-user-pass auth.txt